{
  "name": "chainvault",
  "version": "0.2.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "clean": "rimraf ./build",
    "build": "npm run clean && tsc",
    "start": "node chainvault",
    "local": "ts-node chainvault",
    "local:watch": "nodemon chainvault -e ts,json --exec ts-node chainvault",
    "lint:check": "eslint ./chainvault --ext .ts",
    "lint:fix": "prettier --config prettierrc.json --write chainvault/**/*.ts"
  },
  "devDependencies": {
    "@types/node": "^20.16.10",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^8.10.0",
    "eslint-config-standard": "^17.1.0",
    "nodemon": "^3.1.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "@types/express": "^5.0.0",
    "express": "^5.0.0",
    "winston": "^3.14.2"
  }
}
